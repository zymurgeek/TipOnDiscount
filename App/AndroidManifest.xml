<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.itllp.tipOnDiscount"
      android:versionName="1.1.0" 
      android:versionCode="7">
	<uses-sdk android:minSdkVersion="7" android:targetSdkVersion="19" />
    <application
        android:name="TipOnDiscountApplication" 
        android:label="@string/app_name" 
        android:icon="@drawable/ic_launcher_tipondiscount" 
        android:theme="@style/Theme.AppCompat"
        android:allowBackup="true">
        <activity android:name=".TipOnDiscount"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SetDefaultsActivity"
                  android:label="@string/set_defaults_name"
                  android:parentActivityName=".TipOnDiscount">
        </activity>
        </application>
</manifest> 